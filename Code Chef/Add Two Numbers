// Add Two Numbers
import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        while(n-->0){
            int x = sc.nextInt();
            int y = sc.nextInt();
            System.out.println((x+y)+"");
        }
    }
}
